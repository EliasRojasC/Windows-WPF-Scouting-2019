<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:scoutingProject.Styles">
    <Color x:Key="DefaultGreen">#3FEEE6</Color>
    <SolidColorBrush x:Key="DefaultGreenBrush" Color="{StaticResource DefaultGreen}" />

    <Color x:Key="CoralRed">#FC4445</Color>
